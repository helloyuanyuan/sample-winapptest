        <Call method="WithVehicleType"/>
      </UseString>

      <UseString id="Manufacturer">
        <Call method="WithManufacturer"/>
      </UseString>

      <UseString id="Model">
        <Call method="WithModel"/>
      </UseString>

      <UseString id="ModelVariant">
        <Call method="WithVariant"/>
      </UseString>

      <UseString id="EngineType">
        <Call method="WithEngineType"/>
      </UseString>

      <UseMeasure id="CylinderCapacity">
